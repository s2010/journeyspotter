# JourneySpotter Environment Configuration
# Copy this file to .env and fill in your actual values

# Groq LLM Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_TEMPERATURE=0.1
GROQ_MAX_TOKENS=1000
GROQ_TIMEOUT=30

# OCR Configuration
OCR_LANGUAGES=["en"]
OCR_USE_GPU=false
OCR_TESSERACT_CMD=/usr/bin/tesseract

# Video Processing Configuration
VIDEO_MAX_FRAMES=10
VIDEO_FRAME_SAMPLING=uniform

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=false
API_CORS_ORIGINS=["*"]

# UI Configuration
UI_API_BASE_URL=http://localhost:8000
UI_PORT=8501

# Application Configuration
APP_NAME=JourneySpotter
VERSION=1.0.0
LOG_LEVEL=INFO
MAX_FILE_SIZE=52428800
TEMP_DIR=/tmp
